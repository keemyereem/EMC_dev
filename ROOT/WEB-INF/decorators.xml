<?xml version="1.0" encoding="UTF-8"?>

<decorators defaultdir="/decorators">
	<excludes>
		<pattern>*Ajax.do*</pattern>
		<pattern>*.jsp*</pattern>
		<pattern>/cmm/file/fileDown.do</pattern>
	</excludes>

	<decorator name="dummy" page="/WEB-INF/view/decorators/dummy_decorator.jsp">
		<pattern>/cmm/dummypage.do*</pattern>
	</decorator>

	<decorator name="front" page="/WEB-INF/view/decorators/front_decorator.jsp">
		<pattern>/index.do*</pattern>
		<pattern>/sys/*.do*</pattern>
		<pattern>/sys/**/*.do*</pattern>
		<pattern>/en/*.do*</pattern>
		<pattern>/en/**/*.do*</pattern>
		<pattern>/*.do*</pattern>
		<pattern>/**/*.do*</pattern>
	</decorator>

	<decorator name="log" page="/WEB-INF/view/decorators/log_decorator.jsp">
		<pattern>/login/*.do*</pattern>
	</decorator>

	<decorator name="error" page="/WEB-INF/view/decorators/error_decorator.jsp">
		<pattern>/error/*.do*</pattern>
	</decorator>



	<decorator name="pop" page="/WEB-INF/view/decorators/pop_decorator.jsp">
		<pattern>*popup.do*</pattern>
		<pattern>*layer.do*</pattern>
	</decorator>
</decorators>